{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": {
    "type": "object",
    "properties": {
      "aboutme": {
        "type": "string"
      },
      "email": {
        "type": "string",
        "format": "email"
      },
      "image": {
        "type": "string",
        "format": "uri"
      },
      "name": {
        "type": "string"
      },
      "phone": {
        "type": "string",
        "pattern": "^\\(\\d{3}\\)\\d{3}-\\d{4}$"
      }
    },
    "required": ["aboutme", "email", "image", "name", "phone"]
  }
}
